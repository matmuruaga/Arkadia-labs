{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "script-src 'self' 'wasm-unsafe-eval' 'inline-speculation-rules' https://www.googletagmanager.com https://assets.calendly.com https://unpkg.com https://elevenlabs.io;"
        }
      ]
    }
  ]
}